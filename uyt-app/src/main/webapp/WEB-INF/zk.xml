<?xml version="1.0" encoding="UTF-8"?>
<zk>
  <config-name>app-config</config-name>
  <system-config>
    <label-location>/WEB-INF/i18n/common.properties</label-location>
    <label-location>/WEB-INF/i18n/error.properties</label-location>
    <label-location>/WEB-INF/i18n/login.properties</label-location>
    <label-location>/WEB-INF/i18n/register.properties</label-location>
    <label-location>/WEB-INF/i18n/about.properties</label-location>
    <label-location>/WEB-INF/i18n/cocktail.properties</label-location>
    <label-location>/WEB-INF/i18n/cocktailEntry.properties</label-location>
    <label-location>/WEB-INF/i18n/settings.properties</label-location>
  </system-config>

  <desktop-config>
    <theme-uri>/css/main.css</theme-uri>
  </desktop-config>

  <listener>
    <listener-class>com.github.uyt.ui.configuration.ZkInit</listener-class>
  </listener>

  <library-property>
    <name>org.zkoss.web.preferred.locale</name>
    <value>lt_LT</value>
  </library-property>
</zk>
